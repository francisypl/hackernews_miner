{
    "source": {
        "name": "hacker-news",
        "base": "https://hacker-news.firebaseio.com/v0/",
        "topStories": "topstories.json",
        "item": "item/",
        "method": "GET",
        "timeout": 10000,
        "followRedirect": true,
        "maxRedirects": 10
    },
    "upload": {
        "story": "https://mineranker.herokuapp.com/api/v1/story",
        "maxStories": 10
    },
    "request": {
        "interval": 1800000,
        "success": [200],
        "retries": 5
    },
    "logger": {
        "dir_path": "logs",
        "dev-level": "verbose",
        "prod-level": "info"
    },
    "env": "production",
    "context": {
        "dir_path": "context",
        "filename": "visited.json"
    }
}
